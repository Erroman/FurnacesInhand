<Application x:Class="FurnacesInHand.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FurnacesInHand"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="VoltageCanvas" TargetType="Canvas">
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="VerticalAlignment" Value="Stretch"/>
            <Setter Property="Background" Value="AliceBlue"/>
        </Style>
        <Style x:Key="CurrentCanvas"/>
        <Style x:Key="VacuumCanvas"/>
        <Style x:Key="SolenoidUCanvas"/>
        <Style x:Key="SolenoidICanvas"/>
    </Application.Resources>
</Application>
